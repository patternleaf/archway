{
	"manifest_version": 2,
	"name": "Archway",
	"version": "1",
	"minimum_chrome_version": "24",
	"sockets": { "tcpServer": { "listen": "127.0.0.1:7890" } },
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	},
	"permissions": [ 
		"webview", 
		"storage",
		{ "socket" : ["tcp-listen"] },
		"http://localhost/"
	]
}
